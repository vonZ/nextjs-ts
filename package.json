{
  "name": "nextjs-ts-template",
  "url": "git@github.com:https://github.com/vonZ/nextjs-ts.git",
  "version": "1.0.0",
  "description": "Template for nextjs and typescript",
  "scripts": {
    "start": "next",
    "build": "next build",
    "precommit": "lint-staged",
    "tslint": "tslint -p . -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "author": "Viktor von Zeipel",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@testing-library/react": "^10.0.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.5.2",
    "@types/react": "^16.9.19",
    "@types/styled-components": "^5.0.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.1.0",
    "babel-plugin-emotion": "^10.0.13",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^2.0.1",
    "react-test-renderer": "^16.9.0",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/next": "^9.0.0",
    "@types/react-lazyload": "^2.6.0",
    "@types/react-transition-group": "^4.2.4",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-transform-optional-chaining": "^7.0.0-beta.3",
    "isomorphic-fetch": "^2.2.1",
    "next": "^9.0.6",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.5.5",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.6.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0",
    "styled-components": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
